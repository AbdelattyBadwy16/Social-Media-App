{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "main.jsx", "pages/Register.tsx", "Context/UserContext.jsx", "compontes/Spinner.jsx", "pages/Login.jsx", "Helper/RequireAuth.jsx", "Helper/SaveAuth.jsx", "compontes/Profile/Activity.jsx", "compontes/Profile/NewPost.jsx", "compontes/Members/MemberCard.tsx", "compontes/Profile/Setting.jsx", "compontes/Home/MyFavourite.jsx", "pages/Profile.jsx"],
    "exclude": ["node_modules"]
  }